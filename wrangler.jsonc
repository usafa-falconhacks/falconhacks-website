/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "falconhacks",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-10-04",
  "keep_vars": true,
  "routes": [
    {
      "pattern": "www.falconhacks.org",
      "custom_domain": true
    },
    {
      "pattern": "falconhacks.org",
      "custom_domain": true
    }
  ],
  "compatibility_flags": [
    "nodejs_compat",
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist",
  },
  "observability": {
    "enabled": true,
  },
  "placement": {
    "mode": "smart",
  },
  // "d1_databases": [
